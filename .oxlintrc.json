{
  "plugins": ["import", "typescript", "unicorn", "promise"],
  "ignorePatterns": ["**/lib"],
  "rules": {
    "eslint/no-unused-vars": "off",
    "no-console": "error",
    "unicorn/no-useless-promise-resolve-reject": "error",
    "import/no-cycle": [
      "error",
      {
        "ignoreTypes": true
      }
    ]
  }
}
