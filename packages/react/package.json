{
    "name": "@milkdown/react",
    "version": "6.0.0-next.0",
    "type": "module",
    "main": "./lib/index.es.js",
    "types": "./lib/index.d.ts",
    "sideEffects": false,
    "license": "MIT",
    "scripts": {
        "start": "concurrently -n es,dts \"vite build --watch\"  \"tsc --emitDeclarationOnly --watch\"",
        "test": "vitest",
        "tsc": "tsc --noEmit",
        "build": "vite build && tsc --emitDeclarationOnly"
    },
    "files": [
        "lib",
        "src"
    ],
    "keywords": [
        "milkdown",
        "milkdown plugin",
        "react"
    ],
    "dependencies": {
        "@milkdown/utils": "workspace:*",
        "nanoid": "^3.1.25",
        "tslib": "^2.3.1"
    },
    "peerDependencies": {
        "react": "*",
        "react-dom": "*",
        "@milkdown/core": "^6.0.0-next.0",
        "@milkdown/prose": "^6.0.0-next.0"
    },
    "devDependencies": {
        "@milkdown/core": "workspace:*",
        "@milkdown/prose": "workspace:*",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0"
    },
    "nx": {
        "targets": {
            "build": {
                "outputs": [
                    "packages/react/lib"
                ],
                "dependsOn": [
                    {
                        "target": "build",
                        "projects": "dependencies"
                    }
                ]
            },
            "tsc": {
                "outputs": [],
                "dependsOn": [
                    {
                        "target": "build",
                        "projects": "dependencies"
                    }
                ]
            }
        }
    }
}
