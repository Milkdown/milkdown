{
  "name": "@milkdown/core",
  "type": "module",
  "version": "7.10.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Milkdown/milkdown.git",
    "directory": "packages/core"
  },
  "keywords": [
    "milkdown",
    "prosemirror",
    "remark",
    "markdown",
    "wysiwyg",
    "text editor",
    "markdown editor"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "publishConfig": {
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
      ".": {
        "import": "./lib/index.js",
        "types": "./lib/index.d.ts"
      }
    }
  },
  "scripts": {
    "build": "vite build"
  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@milkdown/ctx": "workspace:*",
    "@milkdown/prose": "workspace:*",
    "@milkdown/transformer": "workspace:*",
    "@milkdown/exception": "workspace:*",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "tslib": "^2.8.1",
    "unified": "^11.0.3"
  }
}
